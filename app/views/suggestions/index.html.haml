.container
  .row
    %h1== Redevelopment Ideas #{@property.address}
    %h3 Rate them, or give us feedback
    .span8
      - @property.suggestions.each do |suggestion|
        = render :partial => 'suggestion', :locals => { :suggestion => suggestion, :property => @property }
  .row
    %center= link_to 'Back to property', property_path(@property)

.container-fluid
  .row.suggestion
    .span12
      %br
      %h1== Ideas Submitted to Rescue #{@property.address}
      %h2 Rate them, or give us feedback
  .row.suggestion
    - @property.suggestions.each do |suggestion|
      = render :partial => 'suggestion', :locals => { :suggestion => suggestion, :property => @property }
  .row
    .span12
      %br
      = link_to 'Back to property', property_path(@property), :class => 'btn btn-inverse btn-large'
